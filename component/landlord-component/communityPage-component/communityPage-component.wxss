.community .weui-search-bar{
  border:none;
  background-color: transparent;
}
.community .mine{
  height: 100rpx;
  width: 100rpx;
  border-radius: 50rpx;
  background: linear-gradient(to bottom right, #54b5b7 , #67deb5);
  font-size:50rpx;
  font-weight: bold;
  color:#fff;
  position: fixed;
  bottom:320rpx;
  right:50rpx;
  overflow: hidden;
  transition:width 0.5s ease-in-out;
}
.community .add{
  height: 100rpx;
  border-radius: 50rpx;
  background: linear-gradient(to bottom right, #54b5b7 , #67deb5);
  font-size:50rpx;
  font-weight: bold;
  color:#fff;
  position: fixed;
  bottom:200rpx;
  right:50rpx;
  overflow: hidden;
  transition:width 0.5s ease-in-out;
}
.community .mine view{
  width: 100rpx;
  border-radius:50rpx;
  text-align: center;
  font-size:40rpx;
  line-height: 100rpx;
}
.community .add_long{
width:430rpx;
}
.community .add_short{
width:100rpx;
}
.community .add view{
  width: 100rpx;
  border-radius:50rpx;
  text-align: center;
  font-size:40rpx;
}
.community .add navigator{
  width: 100rpx;
  border-radius:50rpx;
  text-align: center;
  font-size:40rpx;
}
.community .add navigator p{
  font-size: 18rpx;
}
.community .add .add-icon{
  display:inline-block;
  position: absolute;
  right:0rpx;
  top:0rpx;
  line-height: 100rpx; 
}
.community .add .add-active{
  display:inline-block;
  position: absolute;
  right:100rpx;
  top:0rpx;
  line-height: 30rpx;
  margin-top:20rpx;
}
.community .add .add-text{
  display:inline-block;
  position: absolute;
  right:200rpx;
  top:0rpx;
  line-height: 30rpx;
  margin-top:20rpx;
}
.community .add .add-management{
  display:inline-block;
  position: absolute;
  right:300rpx;
  top:0rpx;
  line-height: 30rpx;
  margin-top:20rpx;
}
.community .header{
  position: fixed;
  top:0px;
  left:0px;
  width: 100%;
  background: #f8f8f8;
}
.community .header .tab{
  height: 50rpx;
  width: 100%;
}
.community .header .tab view{
  width: calc(50% - 40rpx);
  height: 50rpx;
  line-height: 50rpx;
  font-size: 20rpx;
  float:left;
  text-align: center;
  color:#808080;
  margin:0 20rpx;
}
.community .header .tab .selected{
  border-bottom:1px solid #54b5b7;
  color:#54b5b7
}

/* articles page */
.community .community_main{
  margin-top:160rpx;
}
.community .community_main .articles .box{
  border:1px solid red;
  padding:20rpx;
}
.community .community_main .articles .box .top{
  height: 60rpx;
}
.community .community_main .articles .box .top view{
  float:left;
}
.community .community_main .articles .box .top .img{
  width:60rpx;
  height: 60rpx;
  border-radius:50rpx;
  overflow: hidden;
  border:1px solid red;
}

.community .community_main .articles .box .top .img image{
  width: 60rpx;
  height: 60rpx;
}
.community .community_main .articles .box .top .name{
  line-height: 60rpx;
  font-size: 30rpx;
  color:#808080;
  padding-left:30rpx;
}
.community .community_main .articles .box .center{

}
.community .community_main .articles .box .center .txt{
  font-size:30rpx;
  color:#333;
}
