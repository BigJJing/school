/* pages/landlord-mine/info-room/info-room.wxss */
page{
  background: #f8f8f8;
}
.ir-menu{
  padding-bottom:100px;
}
.footer-btn{
  position: fixed;
  bottom:0;
  left:0;
  width: 100%;
}
.footer-btn button{ 
  background-color:#fff;
}
.footer-btn button .add-icon{
  font-weight:400;
  padding-right:10px;
  font-size:20px;
  color:#FCDC3F;
}
.footer-btn button .add-font{
  font-size:18px;
  color:#000;
}
.set{
  padding:10px 15px;
  background-color: #fff;
  font-size:15px;
  line-height: 1.47;
  border-bottom:1px solid #f8f8f8;
}

.set .bd .info text{
  color:#808080;
}
.set .bd .info .type{
  display: inline-block;
  width:80px;
}
.set .ft{
  padding-right:13px;
  color:#808080;
  float:right;
  position: relative;
}
.set .ft::after{
  content:"";
  display: inline-block;
  width:6px;
  height: 6px;
  border-top:2px solid #C8C8CD;
  border-right:2px solid #C8C8CD;
  transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
  position: absolute;
  margin-top:-25px;
  right:2px;
}

